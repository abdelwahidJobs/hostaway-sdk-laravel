{
    "name": "abdelwahidjobs/hostaway-sdk-laravel",
    "description": "A Laravel wrapper for the Hostaway API",
    "license": "MIT",
    "namespace": "AbdelwahidJobs\\HostawaySdkLaravel",
    "library": "HostawaySdkLaravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Backend\\HostawaySdkLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "abdelwahidJobs",
            "email": "abdoo.max@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/data-transfer-object": "^2.8",
        "illuminate/support": "^8.83",
        "guzzlehttp/guzzle": "^7.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backend\\HostawaySdkLaravel\\HostawayServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.47"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Backend\\HostawaySdkLaravel\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/WeChalet/php-vcr"
        }
        ]

}
